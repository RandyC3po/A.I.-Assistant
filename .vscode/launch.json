{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "React Native: Android",
            "request": "launch",
            "type": "reactnative",
            "cwd": "${workspaceFolder}",
            "platform": "android",
            "port": 8081 
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
            "justMyCode": true 
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "my_app.main",
            "console": "integratedTerminal"
        }
    ]
}